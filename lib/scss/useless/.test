.grid, [class*=grid-auto] {
  display: grid;
  gap: 0;
}

.grid {
  grid-template-columns: repeat(12, 1fr);
}
.grid > * {
  grid-column-end: span 12;
}

.grid-auto\:xs, .grid-auto\:sm, .grid-auto\:md,
.grid-auto\:lg, .grid-auto\:xl, .grid-auto\:xxl {
